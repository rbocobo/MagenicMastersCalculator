<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.magenicmasters.calculator"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- <uses-sdk -->
    <!-- android:minSdkVersion="10" -->
    <!-- android:targetSdkVersion="19" /> -->

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.magenicmasters.calculator.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.magenicmasters.calculator.CalculatorActivity"
            android:label="@string/title_activity_calculator"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        </activity>
        <activity
            android:name="com.magenicmasters.calculator.ResultDisplayActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        </activity>
        <activity
            android:name="com.magenicmasters.calculator.ReadDataActivity"
            android:label="@string/title_activity_read_data"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        </activity>
    </application>

</manifest>
